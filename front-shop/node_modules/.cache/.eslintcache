t={OrderList}/>\n    </Switch>\n   \n    \n    <Move/>\n    \n   </div>\n  );\n}\n\nexport default App;\n","/home/seung/react/shop/front-shop/src/reportWebVitals.js",[],"/home/seung/react/shop/front-shop/src/modules/index.js",[],"/home/seung/react/shop/front-shop/src/modules/user.js",[],"/home/seung/react/shop/front-shop/src/components/Login.js",["111"],"import {loginAction} from '../modules/user'\nimport axios from 'axios'\nimport React, {useState} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Title from './Title'\n\n\nfunction Login(){\n    const dispatch = useDispatch();\n    const [userData, setUserData] = useState({\n        username: \"\",\n        password: \"\"\n    })\n    const {username, password} = userData;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setUserData({\n            ...userData,\n            [name]: value\n        })\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            username: username,\n            password: password\n        };\n        axios.post('/apis/v1/user/login', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"로그인 성공\")\n            dispatch(loginAction(response.data.payload.payload))\n            alert(\"로그인 성공\")\n        }).catch(e =>{\n            alert(\"로그인 실패\")\n            console.log(\"로그인 실패\")\n        })\n\n        setUserData({\n            username: \"\",\n            password: \"\"\n        })\n    }\n\n    return (\n        <div>\n    \n        <Title title=\"LOGIN\"/>\n        <Container >\n            <Row className=\"justify-content-md-center\">\n                <Col lg=\"8\">\n                <Form onSubmit={onClickHandler} >\n\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>ID</Form.Label>\n                <Form.Control \n                name = 'username'\n                value = {username}\n                onChange={onChangeHandler}\n                placeholder=\"ID를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>PASSWORD</Form.Label>\n                <Form.Control\n                name = 'password'\n                value = {password} \n                onChange={onChangeHandler}\n                placeholder=\"비밀번호를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n           \n            <Button type=\"submit\">Login</Button>\n            </Form>\n                </Col>\n\n            </Row>\n            \n        </Container>\n        </div>\n    );\n    \n\n    // return (<div style ={{\n    //     display : 'flex', justifyContent : 'center', alignItems: 'center',\n    //     width : '100%', height : '100vh'\n    // }}>\n    //     <form style ={{display : 'flex', flexDirection:'column'}}\n    //         onSubmit={onClickHandler}>\n    //         <label>username</label>\n    //         <input name = \"username\" value = {username} onChange={onChangeHandler}/>\n    //         <label>Password</label>\n    //         <input name = \"password\" value = {password} onChange={onChangeHandler}/>\n    //         <br/>\n    //         <button type = 'submit'>\n    //             Login\n    //         </button>\n    //     </form>\n    // </div>);\n}\n\nexport default Login;","/home/seung/react/shop/front-shop/src/components/useAsync.js",["112","113","114","115"],"import React, { useEffect } from 'react'\nimport {useState, useReducer} from 'react'\nimport asyncReducer from '../components/asyncRuducer'\n\nfunction useAsync(callback, deps = []){\n    const [state, dispatch] = useReducer(asyncReducer, {\n        loading: false,\n        data: null,\n        error: null,\n    })\n    const fetchData = async () =>{\n        dispatch({\n            type: 'LOADING'\n        })\n        try{\n            const data = await callback();\n            dispatch({\n                type: \"SUCCESS\",\n                data: data\n            });\n        }\n        catch(e){\n          \n            dispatch({\n                type: 'ERROR',\n                error: e\n            })\n            console.log(e);\n        }\n        \n    }\n    \n    useEffect(() => {\n        fetchData();\n    }, deps)\n\n    return [state, fetchData];\n\n\n} \n\nexport default useAsync;",["116","117"],"/home/seung/react/shop/front-shop/src/components/asyncRuducer.js",[],"/home/seung/react/shop/front-shop/src/config.js",[],["118","119"],"/home/seung/react/shop/front-shop/src/components/Logout.js",["120"],"import React, {useEffect} from 'react'\nimport axios from 'axios';\nimport {logoutAction} from '../modules/user'\nimport {useDispatch} from 'react-redux'\nimport {Container, Button, Col, Row} from 'react-bootstrap'\nimport Title from './Title'\nfunction Logout({history}) {\n    const dispatch = useDispatch();\n\n    const onClickHandler = () => {\n        axios.get('/apis/v1/user/logout')\n        .then(response => {\n            console.log('로그아웃 합니다.')\n            dispatch(logoutAction())\n            alert('로그아웃 합니다.')\n            history.replace('/')\n        })\n        .catch(e => {\n            console.log('로그아웃 실패 에러')\n            alert('로그아웃 실패')\n        })\n    }\n    return (\n        <div >\n            <Container>\n            <Title title=\"LOGOUT\"></Title>\n            <Row className=\"justify-content-md-center\"> \n                    <Col xs lg=\"8\">\n                    <h3>정말 로그아웃 하시겠습니까?</h3>\n                    <br/>\n                    <br/>\n                    \n                    <Button onClick = {onClickHandler}>로그아웃</Button>\n                    <br/>\n                    \n                    </Col>\n                </Row>\n            \n            </Container>\n          \n        </div>\n    )\n}\n\nexport default Logout;","/home/seung/react/shop/front-shop/src/components/Register.js",["121","122","123"],"import React, {useState} from 'react'\nimport axios from 'axios'\nimport {useSelector, useDispatch} from 'react-redux'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Banner from './Banner'\nimport Title from './Title'\n\n\nfunction Register(){\n    const dispatch = useDispatch();\n    const [userData, setUserData] = useState({\n        username: \"\",\n        password: \"\",\n        email: \"\"\n    })\n    const {username, password, email} = userData;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setUserData({\n            ...userData,\n            [name]: value\n        })\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            username: username,\n            password: password,\n            email: email\n        };\n        axios.post('/apis/v1/user/', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"회원가입 성공\")\n            // dispatch(loginAction(response.data.payload))\n            alert(\"회원가입 성공\")\n        }).catch(e =>{\n            alert(\"회원가입 실패\")\n            console.log(\"회원가입 실패\")\n        })\n\n        setUserData({\n            username: \"\",\n            password: \"\",\n            email: \"\"\n        })\n    }\n    return (\n        <div>\n        \n        <Title title=\"REGISTER\"/>\n        <Container>\n            <Row className=\"justify-content-md-center\"> \n                <Col xs lg={8}>\n                <Form onSubmit={onClickHandler} >\n\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>ID</Form.Label>\n                <Form.Control \n                name = 'username'\n                value = {username}\n                onChange={onChangeHandler}\n                placeholder=\"ID를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>EMAIL</Form.Label>\n                <Form.Control \n                name = 'email'\n                value = {email}\n                onChange={onChangeHandler}\n                placeholder=\"email을 입력해주세요\" />\n            </Form.Group>\n            <br/>\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>PASSWORD</Form.Label>\n                <Form.Control\n                name = 'password'\n                value = {password} \n                onChange={onChangeHandler}\n                placeholder=\"비밀번호를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n           \n            <Button type=\"submit\">Register</Button>\n            </Form>\n                \n                </Col>\n            </Row>\n            \n        </Container>\n        </div>\n   \n    );\n\n    // return (<div style ={{\n    //     display : 'flex', justifyContent : 'center', alignItems: 'center',\n    //     width : '100%', height : '100vh'\n    // }}>\n    //     <form style ={{display : 'flex', flexDirection:'column'}}\n    //         onSubmit={onClickHandler}>\n    //         <label>ID</label>\n    //         <input name = \"username\" value = {username} onChange={onChangeHandler}/>\n    //         <label>Email</label>\n    //         <input name = \"email\" value = {email} onChange={onChangeHandler}/>\n    //         <label>Password</label>\n    //         <input name = \"password\" value = {password} onChange={onChangeHandler}/>\n            \n    //         <br/>\n    //         <button type = 'submit'>\n    //             Register\n    //         </button>\n    //     </form>\n    // </div>);\n}\n\nexport default Register;","/home/seung/react/shop/front-shop/src/components/Home.js",["124","125"],"/home/seung/react/shop/front-shop/src/components/Banner.js",["126","127"],"import React from 'react'\nimport {Container,Row, Col, Nav, Navbar, Form, FormControl, Button} from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\nimport CategoryBanner from './CategoryBanner'\nconst Banner = () => {\n    return(\n  \n  <>\n  <hr/>\n  <br />\n<div>\n<Container>\n      <Row>\n        <Col xs lg = {3}>\n        <Navbar.Brand href=\"#home\">\n          <Link to=\"/\" style={{textDecoration:'none', color:'inherit'}}>\n          <h2>\n            Shopping Mall\n            </h2>\n            </Link>\n            \n            </Navbar.Brand>\n        </Col>\n        <Col xs lg={6}>\n        <Form>\n      <FormControl type=\"text\" placeholder=\"Search\" className=\" mr-sm-2\"/>\n      </Form>\n      \n   \n        </Col>\n\n        <Col xs lg={3}>\n        <Button variant=\"outline-primary\">Search</Button>\n        </Col>\n\n        \n      </Row>\n      </Container>\n    \n   \n</div>\n\n\n</>\n\n        \n     \n    )\n}\n\nexport default Banner;","/home/seung/react/shop/front-shop/src/components/ProductRegister.js",["128","129","130","131"],"import React, {useState} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport axios from 'axios'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Banner from './Banner'\nimport Title from './Title'\n\nfunction ProductRegister(){\n\n    const [product, setProduct] = useState({\n        name: \"\",\n        category_id: null,\n        price: null,\n        quantity: null,\n        description: \"\",\n\n\n    })\n\n    \n    const {name, category_id, price, quantity, description} = product;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setProduct({\n            ...product,\n            [name]: value\n        })\n        console.log(product)\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            seller_id: 4,\n            name: name,\n            category_id: 2,\n            price: price,\n            quantity: quantity,\n            description: description,\n        };\n        \n        axios.post('/apis/v1/product/', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"상품 등록 성공\")\n            // dispatch(loginAction(response.data.payload))\n            alert(\"상품 등록 성공\")\n        }).catch(e =>{\n            alert(\"상품 등록 실패\")\n            console.log(\"상품 등록 실패\")\n        })\n\n    }\n    \n    return(<div>\n        <Title title=\"REGISTER PRODUCT\"></Title>\n        <Container>\n            <Row className=\"justify-content-md-center\">\n                <Col xs lg={8}>\n                <Form onSubmit={onClickHandler} >\n\n            <Form.Group controlId=\"exampleForm.ControlSelect1\">\n                <Form.Label>카테고리</Form.Label>\n                <Form.Control as=\"select\">\n                <option>1</option>\n                <option>2</option>\n                <option>3</option>\n                <option>4</option>\n                <option>5</option>\n                </Form.Control>\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>상품명</Form.Label>\n                <Form.Control \n                name = 'name'\n                value = {name}\n                onChange={onChangeHandler}\n                placeholder=\"상품명을 적어주세요\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>가격</Form.Label>\n                <Form.Control\n                name = 'price'\n                value = {price} \n                onChange={onChangeHandler}\n                placeholder=\"가격을 적어주세요\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>수량</Form.Label>\n                <Form.Control\n                name = 'quantity'\n                value = {quantity} \n                onChange={onChangeHandler}\n                placeholder=\"수량을 적어주세요\" />\n            </Form.Group>\n            {/* \n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>사진</Form.Label>\n                <Form.Control placeholder=\"name@example.com\" />\n            </Form.Group> */}\n\n\n\n            <Form.Group controlId=\"exampleForm.ControlTextarea1\">\n                <Form.Label>상품 설명</Form.Label>\n                <Form.Control \n                as=\"textarea\" \n                rows={5}\n                onChange={onChangeHandler}\n                name = 'description'\n                value = {description}\n                />\n            </Form.Group>\n            <br/>\n            <Button type=\"submit\">등록</Button>\n            </Form>\n                            \n                </Col>\n            </Row>\n\n        </Container>\n            \n    </div>\n\n    );\n\n}\n\nexport default ProductRegister;","/home/seung/react/shop/front-shop/src/components/Move.js",[],"/home/seung/react/shop/front-shop/src/modules/category.js",[],"/home/seung/react/shop/front-shop/src/components/CategoryBanner.js",["132"],"/home/seung/react/shop/front-shop/src/components/ProductByCategory.js",["133","134"],"import Banner from './Banner'\nimport axios from 'axios'\nimport React, {useEffect, useState} from 'react';\nimport DisplayProducts from './DisplayProduct'\nimport {Container} from 'react-bootstrap'\nimport Title from './Title';\n\nfunction ProductByCategory({match}){\n    console.log(match.params.number)\n    const[products,Setproducts]= useState([])\n    const fetchProducts= async ()=>{\n        await axios.get('/apis/v1/category/' + match.params.number).then(res=> {\n            let product_list = res.data.payload.map((data, index)=> {\n                return {\n                    ...data.fields,\n                    id: data.pk\n                }\n            })\n            Setproducts(product_list);\n            console.log(products)\n        })\n    }\n\n    useEffect(()=>{\n        fetchProducts()\n   \n    },[match.params.number])\n\n    return(<div>\n        <Title title=\"카테고리 상품\" set_middle={false}></Title>\n        <Container>\n        <DisplayProducts products={products}/>\n        </Container>\n       \n          \n        \n    </div>)\n}\n\nexport default ProductByCategory","/home/seung/react/shop/front-shop/src/components/Title.js",[],"/home/seung/react/shop/front-shop/src/components/Top.js",["135"],"import {Container, Row, Col} from 'react-bootstrap'\nimport {useSelector} from 'react-redux'\nimport React from 'react'\nimport Logout from './Logout'\nimport { Link } from 'react-router-dom';\n\nconst Top = ()=>{\n    const {isLoggedIn, userData} = useSelector(state =>({\n            isLoggedIn: state.user.isLoggedIn,\n            userData: state.user.payload\n    }))\n\n\n    if(!isLoggedIn){\n        return(\n            <Container>\n                <br/>\n                <Row>\n                <Col md={{ span: 5, offset: 9 }}>\n                <div>\n                <span><Link to='/login' style={{textDecoration:'none'}}>로그인 / </Link></span>\n                <span><Link to='/logout' style={{textDecoration:'none'}}>회원가입</Link></span>\n                </div>\n                </Col>\n                \n                </Row>\n            </Container>\n            \n        \n        )\n    }\n    else{\n        return(\n            <Container>\n            <br/> \n            <Row>\n            <Col md={{ span: 5, offset: 7 }}>\n            <div>\n            <span >오늘도 오셨군요! {userData.username}님! / </span>\n            <span><Link to='/logout' style={{textDecoration:'none'}}>로그아웃</Link></span>\n            </div>\n            </Col>\n            \n            </Row>\n            </Container>\n\n            \n        )\n    }\n}\n\n\nexport default Top;","/home/seung/react/shop/front-shop/src/components/DisplayProduct.js",[],"/home/seung/react/shop/front-shop/src/components/ProductDetail.js",["136","137"],"import {ListGroup, Container, Button, Form, Row, Col} from 'react-bootstrap'\nimport axios from 'axios'\nimport {useEffect, useState} from 'react'\nimport {useSelector} from 'react-redux'\nimport Title from './Title'\n\nfunction ProductDetail({match}){\n    const [amount, setAmount] = useState(1)\n    const {isLoggedIn, userData} = useSelector(state =>({\n        isLoggedIn: state.user.isLoggedIn,\n        userData: state.user.payload\n    }))\n\n\n\n    const[product,setProduct]= useState({\n    })\n    const fetchProduct= async ()=>{\n        await axios.get('/apis/v1/product/' + match.params.number).then(res=> {\n            console.log(res.data.payload.payload)\n            setProduct(res.data.payload.payload);\n        \n        })\n        .catch(e => {\n            // 정보가 없을 때 처리\n            alert('없는 상품 정보')\n        })\n    }\n\n    const orderProduct= async ()=>{\n        let body = {\n            ...product,\n            user_id: userData.user_id,\n            demand_amount: amount,\n        };\n        console.log(body)\n        await axios.post('/apis/v1/order/' + match.params.number, body).then(res=> {\n            console.log(res.data.payload.payload)\n            alert('주문 성공')\n        })\n        .catch(e => {\n            // 정보가 없을 때 처리\n            alert('주문 실패')\n        })\n    }\n    useEffect(()=>{\n        \n            fetchProduct()\n        \n    },[match.params.number])\n\n    const onClickOrder = () => {\n        if(isLoggedIn === false){\n            alert(\"로그인 후 이용하세요.\")\n            return;\n        }\n        if(amount > product.quantity){\n            alert(\"수량이 잘못 되었습니다. \")\n            return; \n        }\n\n        orderProduct();\n    }\n\n    const onClickWishList = () => {\n        if(isLoggedIn === false){\n            alert(\"로그인 후 이용하세요.\")\n            return;\n        }\n    }\n\n    const onChangeHandler = (e) => {\n        const {value} = e.target\n        if(value < 1){\n            alert(\"올바르지 않습니다.\")\n            return;\n        }\n\n        setAmount(parseInt(value))\n    }\n\n    return(\n        <div>\n        <Title title= {product.name}></Title>\n        <Container>\n            <Row className=\"justify-content-md-center\">\n                <Col xs lg=\"8\">\n                <ListGroup>\n            <ListGroup.Item>{product.category}</ListGroup.Item>\n            <ListGroup.Item>상품명 / {product.name}</ListGroup.Item>\n            <ListGroup.Item>상품 가격 / {product.price}</ListGroup.Item>\n            <ListGroup.Item>상품 수량 / {product.quantity}</ListGroup.Item>\n            <ListGroup.Item>{product.description}</ListGroup.Item>\n            <ListGroup.Item>{product.created_at}</ListGroup.Item>\n            </ListGroup>\n        <br/>\n        <p>구매 수량</p>\n        <input type='number' onChange={onChangeHandler}value={amount}></input>\n        <br/>\n        <br/>\n        <Button onClick={onClickWishList}>장바구니</Button>{' '}\n        <Button onClick={onClickOrder}>구매하기</Button>\n                </Col>\n            </Row>\n            \n        </Container>\n\n        </div>\n        \n    )\n}\n\nexport default ProductDetail","/home/seung/react/shop/front-shop/src/components/OrderList.js",["138"],"import React, {useState, useEffect} from 'react'\nimport axios from 'axios'\nimport {ListGroup, Container} from 'react-bootstrap'\nimport Title from './Title'\nfunction OrderList(){\n\n    const [orders, setOrders] = useState([]) \n    const fetchOrders= async ()=>{\n        await axios.get('/apis/v1/product').then(res=> {\n            let order_list = res.data.payload.map((data, index)=> {\n                return (\n                    <ListGroup.Item key= {index}>{data.fields.name}</ListGroup.Item>\n                )\n            })\n            setOrders(order_list);\n            console.log(orders)\n        })\n    }\n\n    useEffect(()=>{\n        fetchOrders()\n   \n    },[])\n\n    return (<div>\n        <Title title=\"구매 목록\" set_middle={false}></Title>\n        <Con