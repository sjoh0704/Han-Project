tch>\n   \n    \n    <Move/>\n    \n   </div>\n  );\n}\n\nexport default App;\n","/home/seung/react/shop/front-shop/src/reportWebVitals.js",[],"/home/seung/react/shop/front-shop/src/modules/index.js",[],"/home/seung/react/shop/front-shop/src/modules/user.js",[],"/home/seung/react/shop/front-shop/src/components/Login.js",["112"],"import {loginAction} from '../modules/user'\nimport axios from 'axios'\nimport React, {useState} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Title from './Title'\n\n\nfunction Login(){\n    const dispatch = useDispatch();\n    const [userData, setUserData] = useState({\n        username: \"\",\n        password: \"\"\n    })\n    const {username, password} = userData;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setUserData({\n            ...userData,\n            [name]: value\n        })\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            username: username,\n            password: password\n        };\n        axios.post('/apis/v1/user/login', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"로그인 성공\")\n            dispatch(loginAction(response.data.payload.payload))\n            alert(\"로그인 성공\")\n        }).catch(e =>{\n            alert(\"로그인 실패\")\n            console.log(\"로그인 실패\")\n        })\n\n        setUserData({\n            username: \"\",\n            password: \"\"\n        })\n    }\n\n    return (\n        <div>\n    \n        <Title title=\"LOGIN\"/>\n        <Container >\n            <Row className=\"justify-content-md-center\">\n                <Col lg=\"8\">\n                <Form onSubmit={onClickHandler} >\n\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>ID</Form.Label>\n                <Form.Control \n                name = 'username'\n                value = {username}\n                onChange={onChangeHandler}\n                placeholder=\"ID를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>PASSWORD</Form.Label>\n                <Form.Control\n                name = 'password'\n                value = {password} \n                onChange={onChangeHandler}\n                placeholder=\"비밀번호를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n           \n            <Button type=\"submit\">Login</Button>\n            </Form>\n                </Col>\n\n            </Row>\n            \n        </Container>\n        </div>\n    );\n    \n\n    // return (<div style ={{\n    //     display : 'flex', justifyContent : 'center', alignItems: 'center',\n    //     width : '100%', height : '100vh'\n    // }}>\n    //     <form style ={{display : 'flex', flexDirection:'column'}}\n    //         onSubmit={onClickHandler}>\n    //         <label>username</label>\n    //         <input name = \"username\" value = {username} onChange={onChangeHandler}/>\n    //         <label>Password</label>\n    //         <input name = \"password\" value = {password} onChange={onChangeHandler}/>\n    //         <br/>\n    //         <button type = 'submit'>\n    //             Login\n    //         </button>\n    //     </form>\n    // </div>);\n}\n\nexport default Login;","/home/seung/react/shop/front-shop/src/components/useAsync.js",["113","114","115","116"],"import React, { useEffect } from 'react'\nimport {useState, useReducer} from 'react'\nimport asyncReducer from '../components/asyncRuducer'\n\nfunction useAsync(callback, deps = []){\n    const [state, dispatch] = useReducer(asyncReducer, {\n        loading: false,\n        data: null,\n        error: null,\n    })\n    const fetchData = async () =>{\n        dispatch({\n            type: 'LOADING'\n        })\n        try{\n            const data = await callback();\n            dispatch({\n                type: \"SUCCESS\",\n                data: data\n            });\n        }\n        catch(e){\n          \n            dispatch({\n                type: 'ERROR',\n                error: e\n            })\n            console.log(e);\n        }\n        \n    }\n    \n    useEffect(() => {\n        fetchData();\n    }, deps)\n\n    return [state, fetchData];\n\n\n} \n\nexport default useAsync;",["117","118"],"/home/seung/react/shop/front-shop/src/components/asyncRuducer.js",[],"/home/seung/react/shop/front-shop/src/config.js",[],["119","120"],"/home/seung/react/shop/front-shop/src/components/Logout.js",["121"],"import React, {useEffect} from 'react'\nimport axios from 'axios';\nimport {logoutAction} from '../modules/user'\nimport {useDispatch} from 'react-redux'\nimport {Container, Button, Col, Row} from 'react-bootstrap'\nimport Title from './Title'\nfunction Logout({history}) {\n    const dispatch = useDispatch();\n\n    const onClickHandler = () => {\n        axios.get('/apis/v1/user/logout')\n        .then(response => {\n            console.log('로그아웃 합니다.')\n            dispatch(logoutAction())\n            alert('로그아웃 합니다.')\n            history.replace('/')\n        })\n        .catch(e => {\n            console.log('로그아웃 실패 에러')\n            alert('로그아웃 실패')\n        })\n    }\n    return (\n        <div >\n            <Container>\n            <Title title=\"LOGOUT\"></Title>\n            <Row className=\"justify-content-md-center\"> \n                    <Col xs lg=\"8\">\n                    <h3>정말 로그아웃 하시겠습니까?</h3>\n                    <br/>\n                    <br/>\n                    \n                    <Button onClick = {onClickHandler}>로그아웃</Button>\n                    <br/>\n                    \n                    </Col>\n                </Row>\n            \n            </Container>\n          \n        </div>\n    )\n}\n\nexport default Logout;","/home/seung/react/shop/front-shop/src/components/Register.js",["122","123","124"],"import React, {useState} from 'react'\nimport axios from 'axios'\nimport {useSelector, useDispatch} from 'react-redux'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Banner from './Banner'\nimport Title from './Title'\n\n\nfunction Register(){\n    const dispatch = useDispatch();\n    const [userData, setUserData] = useState({\n        username: \"\",\n        password: \"\",\n        email: \"\"\n    })\n    const {username, password, email} = userData;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setUserData({\n            ...userData,\n            [name]: value\n        })\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            username: username,\n            password: password,\n            email: email\n        };\n        axios.post('/apis/v1/user/', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"회원가입 성공\")\n            // dispatch(loginAction(response.data.payload))\n            alert(\"회원가입 성공\")\n        }).catch(e =>{\n            alert(\"회원가입 실패\")\n            console.log(\"회원가입 실패\")\n        })\n\n        setUserData({\n            username: \"\",\n            password: \"\",\n            email: \"\"\n        })\n    }\n    return (\n        <div>\n        \n        <Title title=\"REGISTER\"/>\n        <Container>\n            <Row className=\"justify-content-md-center\"> \n                <Col xs lg={8}>\n                <Form onSubmit={onClickHandler} >\n\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>ID</Form.Label>\n                <Form.Control \n                name = 'username'\n                value = {username}\n                onChange={onChangeHandler}\n                placeholder=\"ID를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>EMAIL</Form.Label>\n                <Form.Control \n                name = 'email'\n                value = {email}\n                onChange={onChangeHandler}\n                placeholder=\"email을 입력해주세요\" />\n            </Form.Group>\n            <br/>\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>PASSWORD</Form.Label>\n                <Form.Control\n                name = 'password'\n                value = {password} \n                onChange={onChangeHandler}\n                placeholder=\"비밀번호를 입력해주세요\" />\n            </Form.Group>\n            <br/>\n           \n            <Button type=\"submit\">Register</Button>\n            </Form>\n                \n                </Col>\n            </Row>\n            \n        </Container>\n        </div>\n   \n    );\n\n    // return (<div style ={{\n    //     display : 'flex', justifyContent : 'center', alignItems: 'center',\n    //     width : '100%', height : '100vh'\n    // }}>\n    //     <form style ={{display : 'flex', flexDirection:'column'}}\n    //         onSubmit={onClickHandler}>\n    //         <label>ID</label>\n    //         <input name = \"username\" value = {username} onChange={onChangeHandler}/>\n    //         <label>Email</label>\n    //         <input name = \"email\" value = {email} onChange={onChangeHandler}/>\n    //         <label>Password</label>\n    //         <input name = \"password\" value = {password} onChange={onChangeHandler}/>\n            \n    //         <br/>\n    //         <button type = 'submit'>\n    //             Register\n    //         </button>\n    //     </form>\n    // </div>);\n}\n\nexport default Register;","/home/seung/react/shop/front-shop/src/components/Home.js",["125","126"],"import React, {useEffect, useState} from 'react';\nimport {useSelector, useDispatch} from 'react-redux'\nimport {setCategory} from '../modules/category'\nimport axios from \"axios\";\nimport Carousel from \"react-bootstrap/Carousel\";\nimport {Container, Navbar} from 'react-bootstrap'\nimport DisplayProducts from './DisplayProduct'\n\n\n\n\nexport default function Home(props){\n    const[products,Setproducts]= useState([])\n\n    const dispatch = useDispatch()\n\n    const fetchProducts= async ()=>{\n        await axios.get('/apis/v1/product').then(res=> {\n            console.log(res)\n            let product_list = res.data.payload.map(data=> {\n\n                return  {\n                    ...data.fields,\n                    id: data.pk\n                }\n            })\n            Setproducts(product_list);\n            console.log(product_list)\n        })\n    }\n    const fetchCategory= async ()=>{\n        await axios.get('/apis/v1/category').then(res=> {\n            let category_list = res.data.payload.map(data=> {\n                return data.fields\n            })\n           \n            console.log(category_list);\n            dispatch(setCategory(category_list))\n        }).catch((e) => {\n            console.log(e)\n        } )\n    }\n\n    useEffect(()=>{\n        fetchProducts();\n        fetchCategory();\n    },[])\n        return (\n            <div>\n                <br/>\n                <div style={{marginTop:30}}>\n                <Navbar bg=\"primary\" variant=\"dark\" />\n                \n                <ControlledCarousel/>\n                <Navbar bg=\"primary\" variant=\"dark\" />\n                <br/>\n                <Container>\n                    <DisplayProducts products={products}/>\n                </Container>\n                \n               \n\n                </div>\n            </div>\n        );\n}\n\nfunction ControlledCarousel() {\n    const [index, setIndex] = useState(0);\n\n    const handleSelect = (selectedIndex, e) => {\n        setIndex(selectedIndex);\n    };\n    return (\n        <Carousel activeIndex={index} onSelect={handleSelect}>\n            <Carousel.Item>\n                <img\n                    className=\"d-block w-100\"\n                    src=\"https://images.unsplash.com/photo-1573612664822-d7d347da7b80?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60\"\n                    alt=\"First slide\"\n                    width=\"800\"\n                    height=\"400\"\n                />\n                <Carousel.Caption>\n                    <h3>First slide label</h3>\n                    <p>First slide label ~~~~</p>\n                </Carousel.Caption>\n            </Carousel.Item>\n            <Carousel.Item>\n                <img\n                    className=\"d-block w-100\"\n                    src=\"https://i.pinimg.com/originals/b1/68/2d/b1682d7c5579f8ce81b8df8088db43a1.jpg\"\n                    alt=\"Second slide\"\n                    width=\"800\"\n                    height=\"400\"\n                />\n\n                <Carousel.Caption>\n                    <h3>Second slide label</h3>\n                    <p>아무거나 적어 </p>\n                </Carousel.Caption>\n            </Carousel.Item>\n            <Carousel.Item>\n                <img\n                    className=\"d-block w-100\"\n                    src=\"https://img.freepik.com/free-vector/big-fashion-shop-super-market-male-clothes-shopping-mall-interior-banner-with-copy-space_48369-11918.jpg?size=626&ext=jpg\"\n                    alt=\"Third slide\"\n                    width=\"800\"\n                    height=\"400\"\n                />\n                <Carousel.Caption>\n                    <h3>Third slide label</h3>\n                    <p>\n                    세번째\n                    </p>\n                </Carousel.Caption>\n            </Carousel.Item>\n        </Carousel>\n    );\n}","/home/seung/react/shop/front-shop/src/components/Banner.js",["127","128"],"import React from 'react'\nimport {Container,Row, Col, Nav, Navbar, Form, FormControl, Button} from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\nimport CategoryBanner from './CategoryBanner'\nconst Banner = () => {\n    return(\n  \n  <>\n  <hr/>\n  <br />\n<div>\n<Container>\n      <Row>\n        <Col xs lg = {3}>\n        <Navbar.Brand href=\"#home\">\n          <Link to=\"/\" style={{textDecoration:'none', color:'inherit'}}>\n          <h2>\n            Shopping Mall\n            </h2>\n            </Link>\n            \n            </Navbar.Brand>\n        </Col>\n        <Col xs lg={6}>\n        <Form>\n      <FormControl type=\"text\" placeholder=\"Search\" className=\" mr-sm-2\"/>\n      </Form>\n      \n   \n        </Col>\n\n        <Col xs lg={3}>\n        <Button variant=\"outline-primary\">Search</Button>\n        </Col>\n\n        \n      </Row>\n      </Container>\n    \n   \n</div>\n\n\n</>\n\n        \n     \n    )\n}\n\nexport default Banner;","/home/seung/react/shop/front-shop/src/components/ProductRegister.js",["129","130","131","132"],"import React, {useState} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport axios from 'axios'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Banner from './Banner'\nimport Title from './Title'\n\nfunction ProductRegister(){\n\n    const [product, setProduct] = useState({\n        name: \"\",\n        category_id: null,\n        price: null,\n        quantity: null,\n        description: \"\",\n\n\n    })\n\n    \n    const {name, category_id, price, quantity, description} = product;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setProduct({\n            ...product,\n            [name]: value\n        })\n        console.log(product)\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            seller_id: 4,\n            name: name,\n            category_id: 2,\n            price: price,\n            quantity: quantity,\n            description: description,\n        };\n        \n        axios.post('/apis/v1/product/', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"상품 등록 성공\")\n            // dispatch(loginAction(response.data.payload))\n            alert(\"상품 등록 성공\")\n        }).catch(e =>{\n            alert(\"상품 등록 실패\")\n            console.log(\"상품 등록 실패\")\n        })\n\n    }\n    \n    return(<div>\n        <Title title=\"REGISTER PRODUCT\"></Title>\n        <Container>\n            <Row className=\"justify-content-md-center\">\n                <Col xs lg={8}>\n                <Form onSubmit={onClickHandler} >\n\n            <Form.Group controlId=\"exampleForm.ControlSelect1\">\n                <Form.Label>카테고리</Form.Label>\n                <Form.Control as=\"select\">\n                <option>1</option>\n                <option>2</option>\n                <option>3</option>\n                <option>4</option>\n                <option>5</option>\n                </Form.Control>\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>상품명</Form.Label>\n                <Form.Control \n                name = 'name'\n                value = {name}\n                onChange={onChangeHandler}\n                placeholder=\"상품명을 적어주세요\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>가격</Form.Label>\n                <Form.Control\n                name = 'price'\n                value = {price} \n                onChange={onChangeHandler}\n                placeholder=\"가격을 적어주세요\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>수량</Form.Label>\n                <Form.Control\n                name = 'quantity'\n                value = {quantity} \n                onChange={onChangeHandler}\n                placeholder=\"수량을 적어주세요\" />\n            </Form.Group>\n            {/* \n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>사진</Form.Label>\n                <Form.Control placeholder=\"name@example.com\" />\n            </Form.Group> */}\n\n\n\n            <Form.Group controlId=\"exampleForm.ControlTextarea1\">\n                <Form.Label>상품 설명</Form.Label>\n                <Form.Control \n                as=\"textarea\" \n                rows={5}\n                onChange={onChangeHandler}\n                name = 'description'\n                value = {description}\n                />\n            </Form.Group>\n            <br/>\n            <Button type=\"submit\">등록</Button>\n            </Form>\n                            \n                </Col>\n            </Row>\n\n        </Container>\n            \n    </div>\n\n    );\n\n}\n\nexport default ProductRegister;","/home/seung/react/shop/front-shop/src/components/Move.js",[],"/home/seung/react/shop/front-shop/src/modules/category.js",[],"/home/seung/react/shop/front-shop/src/components/CategoryBanner.js",["133"],"import React from 'react'\nimport {useSelector} from 'react-redux'\nimport {Container, Navbar, Nav, NavDropdown, Col, Row} from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\nfunction CategoryBanner(){\n    const {kind} = useSelector(state => ({\n        kind: state.category.payload\n    }))\n    console.log(kind)\n    const display_category = kind.map((k, index) => {\n        \n        let  path = '/category/' + (index + 1)\n        return (\n            <NavDropdown.Item key={index}><Link to={path} style={{textDecoration:'none', color:'inherit'}}>{k.kind}</Link></NavDropdown.Item>\n           \n        );\n    }\n    );\n    \n\n    return(\n\n            <div>\n\n          \n                    \n                    <Container>\n                        <Row>\n                            <Col xs lg={3}>\n                            <NavDropdown title=\"Category\" id=\"navbarScrollingDropdown\">\n                 