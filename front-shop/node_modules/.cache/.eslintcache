} onChange={onChangeHandler}/>\n    //         <label>Email</label>\n    //         <input name = \"email\" value = {email} onChange={onChangeHandler}/>\n    //         <label>Password</label>\n    //         <input name = \"password\" value = {password} onChange={onChangeHandler}/>\n            \n    //         <br/>\n    //         <button type = 'submit'>\n    //             Register\n    //         </button>\n    //     </form>\n    // </div>);\n}\n\nexport default Register;","/home/seung/react/shop/front-shop/src/components/Home.js",["127","128"],"import React, {useEffect, useState} from 'react';\nimport {useSelector, useDispatch} from 'react-redux'\nimport {setCategory} from '../modules/category'\nimport axios from \"axios\";\nimport Carousel from \"react-bootstrap/Carousel\";\nimport {Container, Navbar} from 'react-bootstrap'\nimport DisplayProducts from './DisplayProduct'\n\n\n\n\nexport default function Home(props){\n    const[products,Setproducts]= useState([])\n\n    const dispatch = useDispatch()\n\n    const fetchProducts= async ()=>{\n        await axios.get('/apis/v1/product').then(res=> {\n            console.log(res)\n            let product_list = res.data.payload.map(data=> {\n\n                return  {\n                    ...data.fields,\n                    id: data.pk\n                }\n            })\n            Setproducts(product_list);\n            console.log(product_list)\n        })\n    }\n    const fetchCategory= async ()=>{\n        await axios.get('/apis/v1/category').then(res=> {\n            let category_list = res.data.payload.map(data=> {\n                return data.fields\n            })\n           \n            console.log(category_list);\n            dispatch(setCategory(category_list))\n        }).catch((e) => {\n            console.log(e)\n        } )\n    }\n\n    useEffect(()=>{\n        fetchProducts();\n        fetchCategory();\n    },[])\n        return (\n            <div>\n                <br/>\n                <div style={{marginTop:30}}>\n                <Navbar bg=\"primary\" variant=\"dark\" />\n                \n                <ControlledCarousel/>\n                <Navbar bg=\"primary\" variant=\"dark\" />\n                <br/>\n                <Container>\n                    <DisplayProducts products={products}/>\n                </Container>\n                \n               \n\n                </div>\n            </div>\n        );\n}\n\nfunction ControlledCarousel() {\n    const [index, setIndex] = useState(0);\n\n    const handleSelect = (selectedIndex, e) => {\n        setIndex(selectedIndex);\n    };\n    return (\n        <Carousel activeIndex={index} onSelect={handleSelect}>\n            <Carousel.Item>\n                <img\n                    className=\"d-block w-100\"\n                    src=\"https://images.unsplash.com/photo-1573612664822-d7d347da7b80?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60\"\n                    alt=\"First slide\"\n                    width=\"800\"\n                    height=\"400\"\n                />\n                <Carousel.Caption>\n                    <h3>First slide label</h3>\n                    <p>First slide label ~~~~</p>\n                </Carousel.Caption>\n            </Carousel.Item>\n            <Carousel.Item>\n                <img\n                    className=\"d-block w-100\"\n                    src=\"https://i.pinimg.com/originals/b1/68/2d/b1682d7c5579f8ce81b8df8088db43a1.jpg\"\n                    alt=\"Second slide\"\n                    width=\"800\"\n                    height=\"400\"\n                />\n\n                <Carousel.Caption>\n                    <h3>Second slide label</h3>\n                    <p>아무거나 적어 </p>\n                </Carousel.Caption>\n            </Carousel.Item>\n            <Carousel.Item>\n                <img\n                    className=\"d-block w-100\"\n                    src=\"https://img.freepik.com/free-vector/big-fashion-shop-super-market-male-clothes-shopping-mall-interior-banner-with-copy-space_48369-11918.jpg?size=626&ext=jpg\"\n                    alt=\"Third slide\"\n                    width=\"800\"\n                    height=\"400\"\n                />\n                <Carousel.Caption>\n                    <h3>Third slide label</h3>\n                    <p>\n                    세번째\n                    </p>\n                </Carousel.Caption>\n            </Carousel.Item>\n        </Carousel>\n    );\n}","/home/seung/react/shop/front-shop/src/components/Banner.js",["129"],"import React from 'react'\nimport {Container,Row, Col, Nav, Navbar, Form, FormControl, Button} from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\n\nconst Banner = () => {\n    return(\n  \n  <>\n  <hr/>\n  <br />\n<div>\n<Container>\n      <Row>\n        <Col xs lg = {3}>\n        <Navbar.Brand href=\"#home\">\n          <Link to=\"/\" style={{textDecoration:'none', color:'inherit'}}>\n          <h2>\n            Shopping Mall\n            </h2>\n            </Link>\n            \n            </Navbar.Brand>\n        </Col>\n        <Col xs lg={6}>\n        <Form>\n      <FormControl type=\"text\" placeholder=\"Search\" className=\" mr-sm-2\"/>\n      </Form>\n      \n   \n        </Col>\n\n        <Col xs lg={3}>\n        <Button variant=\"outline-primary\">Search</Button>\n        </Col>\n\n        \n      </Row>\n      </Container>\n    \n   \n</div>\n\n\n</>\n\n        \n     \n    )\n}\n\nexport default Banner;","/home/seung/react/shop/front-shop/src/components/ProductRegister.js",["130","131","132","133"],"import React, {useState} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport axios from 'axios'\nimport {Form, Container, Button, Row, Col} from 'react-bootstrap'\nimport Banner from './Banner'\nimport Title from './Title'\n\nfunction ProductRegister(){\n\n    const [product, setProduct] = useState({\n        name: \"\",\n        category_id: null,\n        price: null,\n        quantity: null,\n        description: \"\",\n\n\n    })\n\n    \n    const {name, category_id, price, quantity, description} = product;\n\n    const onChangeHandler = (e) => {\n        const {name, value} = e.target;\n        setProduct({\n            ...product,\n            [name]: value\n        })\n        console.log(product)\n\n    };\n    const onClickHandler = (e)=>{\n        e.preventDefault();\n        let body = {\n            seller_id: 4,\n            name: name,\n            category_id: 2,\n            price: price,\n            quantity: quantity,\n            description: description,\n        };\n        \n        axios.post('/apis/v1/product/', body)\n        .then(response => {\n            // props.history.push('/')\n            console.log(\"상품 등록 성공\")\n            // dispatch(loginAction(response.data.payload))\n            alert(\"상품 등록 성공\")\n        }).catch(e =>{\n            alert(\"상품 등록 실패\")\n            console.log(\"상품 등록 실패\")\n        })\n\n    }\n    \n    return(<div>\n        <Title title=\"REGISTER PRODUCT\"></Title>\n        <Container>\n            <Row className=\"justify-content-md-center\">\n                <Col xs lg={8}>\n                <Form onSubmit={onClickHandler} >\n\n            <Form.Group controlId=\"exampleForm.ControlSelect1\">\n                <Form.Label>카테고리</Form.Label>\n                <Form.Control as=\"select\">\n                <option>1</option>\n                <option>2</option>\n                <option>3</option>\n                <option>4</option>\n                <option>5</option>\n                </Form.Control>\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>상품명</Form.Label>\n                <Form.Control \n                name = 'name'\n                value = {name}\n                onChange={onChangeHandler}\n                placeholder=\"상품명을 적어주세요\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>가격</Form.Label>\n                <Form.Control\n                name = 'price'\n                value = {price} \n                onChange={onChangeHandler}\n                placeholder=\"가격을 적어주세요\" />\n            </Form.Group>\n\n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>수량</Form.Label>\n                <Form.Control\n                name = 'quantity'\n                value = {quantity} \n                onChange={onChangeHandler}\n                placeholder=\"수량을 적어주세요\" />\n            </Form.Group>\n            {/* \n            <Form.Group controlId=\"exampleForm.ControlInput1\">\n                <Form.Label>사진</Form.Label>\n                <Form.Control placeholder=\"name@example.com\" />\n            </Form.Group> */}\n\n\n\n            <Form.Group controlId=\"exampleForm.ControlTextarea1\">\n                <Form.Label>상품 설명</Form.Label>\n                <Form.Control \n                as=\"textarea\" \n                rows={5}\n                onChange={onChangeHandler}\n                name = 'description'\n                value = {description}\n                />\n            </Form.Group>\n            <br/>\n            <Button type=\"submit\">등록</Button>\n            </Form>\n                            \n                </Col>\n            </Row>\n\n        </Container>\n            \n    </div>\n\n    );\n\n}\n\nexport default ProductRegister;","/home/seung/react/shop/front-shop/src/components/Move.js",[],"/home/seung/react/shop/front-shop/src/modules/category.js",[],"/home/seung/react/shop/front-shop/src/components/CategoryBanner.js",["134"],"import React from 'react'\nimport {useSelector} from 'react-redux'\nimport {Container, Navbar, Nav, NavDropdown, Col, Row} from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\nfunction CategoryBanner(){\n    const {kind} = useSelector(state => ({\n        kind: state.category.payload\n    }))\n    console.log(kind)\n    const display_category = kind.map((k, index) => {\n        \n        let  path = '/category/' + (index + 1)\n        return (\n            <NavDropdown.Item key={index}><Link to={path} style={{textDecoration:'none', color:'inherit'}}>{k.kind}</Link></NavDropdown.Item>\n           \n        );\n    }\n    );\n    \n\n    return(\n\n            <div>\n\n          \n                    \n                    <Container>\n                        <Row>\n                            <Col xs lg={3}>\n                            <NavDropdown title=\"Category\" id=\"navbarScrollingDropdown\">\n                            {display_category}\n                            </NavDropdown>\n                            </Col>\n\n                            <Col>\n                            <Nav className=\"mr-auto\">\n            \n                            <Nav.Link ><Link to=\"/register\"style={{textDecoration:'none', color:'inherit'}}>판매하기</Link></Nav.Link>\n                            <Nav.Link ><Link to=\"/orderlist\"style={{textDecoration:'none', color:'inherit'}}>구매목록</Link></Nav.Link>\n                            <Nav.Link ><Link to=\"/register\"style={{textDecoration:'none', color:'inherit'}}>장바구니</Link></Nav.Link>\n                        \n                            </Nav>\n                            </Col>\n\n                            \n                        </Row>\n      \n        </Container>\n            \n          </div>\n  \n      \n        \n   );\n}\n\nexport default CategoryBanner","/home/seung/react/shop/front-shop/src/components/ProductByCategory.js",["135","136"],"import Banner from './Banner'\nimport axios from 'axios'\nimport React, {useEffect, useState} from 'react';\nimport DisplayProducts from './DisplayProduct'\nimport {Container} from 'react-bootstrap'\nimport Title from './Title';\n\nfunction ProductByCategory({match}){\n    console.log(match.params.number)\n    const[products,Setproducts]= use